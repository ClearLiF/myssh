{
  "name": "myssh",
  "version": "1.0.0",
  "description": "SSH 管理工具 - 基于 Electron + Vue 3 + Element Plus",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "preview": "vite preview",
    "start": "electron ."
  },
  "keywords": [
    "ssh",
    "electron",
    "vue",
    "element-plus"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "concurrently": "^8.2.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "vite": "^4.4.9",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "electron-store": "^8.1.0",
    "element-plus": "^2.3.8",
    "hterm-umd": "^1.0.1",
    "node-ssh": "^13.1.0",
    "vue": "^3.3.4"
  },
  "build": {
    "appId": "com.example.myssh",
    "productName": "MySSH",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ]
  }
}
